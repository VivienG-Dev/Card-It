{
  "version": 2,
  "builds": [
    {
      "src": "frontend/nuxt.config.js",
      "use": "@nuxtjs/vercel-builder"
    },
    {
      "src": "backend/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/backend/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "/frontend/.output/"
    }
  ]
}
